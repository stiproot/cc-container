{
  "name": "claude-agent-svc",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node22 --format=esm --outfile=dist/index.js --packages=external --keep-names --log-level=info",
    "build:prod": "esbuild src/index.ts --bundle --platform=node --target=node22 --format=esm --outfile=dist/index.js --packages=external --keep-names --log-level=info --minify",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:unit": "vitest run --exclude '**/integration/**'",
    "test:integration": "vitest run src/__tests__/integration",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "lint:eslint": "eslint --config .eslintrc.cjs . --ext .ts,.js",
    "lint:prettier": "prettier --config .prettierrc.json --check \"src/**/*.{ts,js,json}\"",
    "fix:eslint": "eslint --config .eslintrc.cjs . --ext .ts,.js --fix",
    "fix:prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,js,json}\""
  },
  "keywords": ["claude", "agent", "automation", "mcp", "effect-ts"],
  "author": "",
  "license": "ISC",
  "description": "Containerized Claude Code CLI agent with MCP integration",
  "dependencies": {
    "@effect/platform": "^0.92.1",
    "@effect/platform-node": "^0.98.3",
    "@effect/schema": "^0.75.5",
    "effect": "^3.18.4",
    "express": "^5.1.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.0",
    "@effect/vitest": "^0.26.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv-cli": "^10.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.3.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
